	<Code>
	
	Public Function FormatAmountAndCurrencySymbolString(currencySymbol As String) As String
			
		Dim langCode as String
		langCode = UCase(UserLanguageCode())
		
		Dim englishLangCode as String
		englishLangCode = "EN"
		
		Dim result as String
		If (englishLangCode = langCode) Then
			result = "'" &amp; currencySymbol &amp; "'#,0.00" &amp; ";('" &amp; currencySymbol &amp; "'0.00)"
		Else
			result = "#,0.00 " &amp; "'" &amp; currencySymbol &amp; "';(0.00 " &amp; "'" &amp; currencySymbol &amp; "')"
		End If
		
		Return result
	End Function
				
	Public Function FormatAmountAndCurrencySymbol(amount as Double, currencySymbol As String) As String
			
		Dim langCode as String
		langCode = UCase(UserLanguageCode())
		
		Dim formattedAmount = Format(amount,"#,0.00;(#,0.00)") 
		
		Dim englishLangCode as String
		englishLangCode = "EN"
		
		Dim result as String
		If (englishLangCode = langCode) Then
			result = currencySymbol &amp; formattedAmount
		Else
			result = formattedAmount &amp; " " &amp; currencySymbol
		End If
		
		Return result
	End Function
		
	Public Function UserLanguageCode() As String
		Return Report.User.Language.Substring(0,2)
	End Function
	 
	</Code>
